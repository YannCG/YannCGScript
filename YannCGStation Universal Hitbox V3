    local L_19_ = 'Create' .. L_18_arg1.method
    local L_20_ = L_18_arg1.tab[L_19_]
    if type(L_20_) == 'function' then
        L_20_(L_18_arg1.tab, {
            Name = L_18_arg1.name,
            SectionParent = L_18_arg1.section, 
            CurrentValue = L_18_arg1.value,
            Flag = L_18_arg1.flag,
            Options = L_18_arg1.options,
            CurrentOption = L_18_arg1.currentOption,
            MultipleOptions = L_18_arg1.multipleOptions,
            Range = L_18_arg1.range,
            Color = L_18_arg1.color,
            Increment = L_18_arg1.increment,
            Callback = function(L_21_arg1)
                if L_18_arg1.multipleOptions == false then
                    L_21_arg1 = L_21_arg1[1]
                end
                L_7_:Set(L_18_arg1.flag, L_21_arg1)
            end
        })
    end
end

