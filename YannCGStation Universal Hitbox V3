-- Silent Aim æ ¸å¿ƒåŠŸèƒ½
local function setupSilentAim()
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local UserInputService = game:GetService("UserInputService")
    
    -- è·å–é¼ æ ‡ä½ç½®
    local function getMousePosition()
        return UserInputService:GetMouseLocation()
    end
    
    -- æ£€æŸ¥æ˜¯å¦åœ¨å›¢é˜Ÿä¸­
    local function isEnemy(player)
        if not silentAimTeamCheck then return true end
        
        local localPlayer = Players.LocalPlayer
        if localPlayer.Team and player.Team then
            return localPlayer.Team ~= player.Team
        end
        
        return true
    end
    
    -- æ£€æŸ¥æ˜¯å¦å¯è§
    local function isVisible(target, origin)
        if not silentAimVisibleCheck then return true end
        
        local params = RaycastParams.new()
        params.FilterDescendantsInstances = {Players.LocalPlayer.Character, target.Parent}
        params.FilterType = Enum.RaycastFilterType.Blacklist
        
        local result = workspace:Raycast(origin, target.Position - origin, params)
        return result == nil or result.Instance:IsDescendantOf(target.Parent)
    end
    
    -- è·å–æœ€ä½³ç›®æ ‡
    local function getBestTarget()
        local bestTarget = nil
        local bestDistance = silentAimFOV
        local mousePos = getMousePosition()
        
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= Players.LocalPlayer and player.Character and player.Character:FindFirstChild(silentAimTargetPart) then
                if isEnemy(player) then
                    local targetPart = player.Character:FindFirstChild(silentAimTargetPart)
                    if targetPart and isVisible(targetPart, Players.LocalPlayer.Character.Head.Position) then
                        local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(targetPart.Position)
                        
                        if onScreen then
                            local distance = (Vector2.new(screenPos.X, screenPos.Y) - mousePos).Magnitude
                            
                            if distance < bestDistance and math.random(1, 100) <= silentAimHitChance then
                                bestDistance = distance
                                bestTarget = targetPart
                            end
                        end
                    end
                end
            end
        end
        
        return bestTarget
    end
    
    -- Hook __namecall æ–¹æ³•è¿›è¡Œ Silent Aim
    local mt = getrawmetatable(game)
    local oldNamecall = mt.__namecall
    
    if setreadonly then setreadonly(mt, false) end
    
    mt.__namecall = newcclosure(function(...)
        local method = getnamecallmethod()
        local args = {...}
        
        if silentAimEnabled and method == "FireServer" then
            local remoteName = tostring(args[1])
            
            -- æ£€æµ‹å°„å‡»ç›¸å…³çš„è¿œç¨‹äº‹ä»¶
            if string.find(remoteName:lower(), "shoot") or 
               string.find(remoteName:lower(), "fire") or 
               string.find(remoteName:lower(), "hit") then
                
                local target = getBestTarget()
                if target then
                    -- ä¿®æ”¹å°„å‡»å‚æ•°ä»¥ç„å‡†ç›®æ ‡
                    if args[2] then
                        if typeof(args[2]) == "Vector3" then
                            args[2] = target.Position
                        elseif typeof(args[2]) == "CFrame" then
                            args[2] = CFrame.new(args[2].Position, target.Position)
                        end
                    end
                end
            end
        end
        
        return oldNamecall(unpack(args))
    end)
    
    if setreadonly then setreadonly(mt, true) end
end

-- é˜²è¸¢é˜²å°ç³»ç»Ÿ
local function setupAntiCheat()
    -- é˜²æ­¢è¢«è¸¢å‡º
    if antiKickEnabled then
        local mt = getrawmetatable(game)
        local oldNamecall = mt.__namecall
        
        if setreadonly then setreadonly(mt, false) end
        
        mt.__namecall = newcclosure(function(...)
            local method = getnamecallmethod()
            local args = {...}
            
            -- é˜»æ­¢è¸¢å‡ºç›¸å…³å‡½æ•°
            if method == "Kick" or method == "kick" then
                L_8_:Notify({
                    Title = "Anti-Kick",
                    Content = "Kick attempt blocked!",
                    Duration = 3
                })
                return nil
            end
            
            return oldNamecall(unpack(args))
        end)
        
        if setreadonly then setreadonly(mt, true) end
    end
    
    -- éšè—è„šæœ¬ç—•è¿¹
    if antiDetectionEnabled then
        -- æ¸…é™¤ç—•è¿¹
        if hookfunction then
            local oldInfo = hookfunction(info, function()
                return "Script"
            end)
        end
        
        -- ä¼ªè£…ç½‘ç»œè¯·æ±‚
        if hookfunction then
            local oldHttp = hookfunction(game.HttpGet, function(...)
                return "Blocked"
            end)
        end
    end
    
    -- é˜²æ­¢æ—¥å¿—è®°å½•
    if antiLogEnabled then
        -- è¿™é‡Œå¯ä»¥æ·»åŠ æ›´å¤šé˜²æ—¥å¿—æªæ–½
        print = function(...) end
        warn = function(...) end
    end
end

