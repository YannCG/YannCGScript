local L_30_ = loadstring(L_1_.HttpGet(L_1_, 'https://sirius.menu/sense'))()
L_30_.teamSettings.enemy.enabled = true
L_30_.teamSettings.friendly.enabled = true

local function L_31_(L_32_arg1, L_33_arg1)
    L_30_.teamSettings.enemy[L_32_arg1] = L_33_arg1
    L_30_.teamSettings.friendly[L_32_arg1] = L_33_arg1
end

local function L_34_(L_35_arg1)
    if not L_35_arg1 or not L_35_arg1.type then return end

    local function L_36_(L_37_arg1)
        if not L_35_arg1.props then return end
        local function L_38_(L_39_arg1)
            if L_35_arg1.alpha ~= nil then return {L_39_arg1, L_35_arg1.alpha} end
            return L_39_arg1
        end
        if L_35_arg1.props.friendly then
            local L_40_ = L_30_.teamSettings.friendly
            for L_41_forvar0, L_42_forvar1 in ipairs(L_35_arg1.props.friendly) do
                L_40_[L_42_forvar1] = (L_35_arg1.type == "color") and L_38_(L_37_arg1) or L_37_arg1
            end
        end
        if L_35_arg1.props.enemy then
            local L_43_ = L_30_.teamSettings.enemy
            for L_44_forvar0, L_45_forvar1 in ipairs(L_35_arg1.props.enemy) do
                L_43_[L_45_forvar1] = (L_35_arg1.type == "color") and L_38_(L_37_arg1) or L_37_arg1
            end
        end
    end

    local function L_46_(L_47_arg1)
        if L_35_arg1.setting then
            L_31_(L_35_arg1.setting, L_47_arg1)
        end
        L_36_(L_47_arg1)
        if L_35_arg1.onChange then L_35_arg1.onChange(L_47_arg1) end
    end

    if L_35_arg1.type == "section" then
        L_14_:CreateSection(L_35_arg1.name or "")
        return
    end

    if L_35_arg1.type == "label" then
        L_14_:CreateLabel(L_35_arg1.name or "")
        return
    end

    if L_35_arg1.type == "toggle" then
        return L_14_:CreateToggle({
            Name = L_35_arg1.name,
            CurrentValue = L_35_arg1.default or false,
            Flag = L_35_arg1.flag or "",
            Callback = L_46_
        })
    end

    if L_35_arg1.type == "color" then
        return L_14_:CreateColorPicker({
            Name = L_35_arg1.name,
            Color = L_35_arg1.color or Color3.fromRGB(255,255,255),
            Flag = L_35_arg1.flag or "",
            Callback = L_46_
        })
    end

    if L_35_arg1.type == "dropdown" then
        return L_14_:CreateDropdown({
            Name = L_35_arg1.name,
            Options = L_35_arg1.options or {},
            CurrentOption = L_35_arg1.current,
            Flag = L_35_arg1.flag or "",
            Callback = L_46_
        })
    end

    if L_35_arg1.type == "slider" then
        return L_14_:CreateSlider({
            Name = L_35_arg1.name,
            Range = L_35_arg1.range or {0,100},
            CurrentValue = (L_35_arg1.default ~= nil and L_35_arg1.default) or ((L_35_arg1.range and L_35_arg1.range[1]) or 0),
            Increment = L_35_arg1.increment or 1,
            Suffix = L_35_arg1.suffix or "",
            Flag = L_35_arg1.flag or "",
            Callback = L_46_
        })
    end
end

